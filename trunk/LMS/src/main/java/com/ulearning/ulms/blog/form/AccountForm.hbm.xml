<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
        <!--
            Created by the Middlegen Hibernate plugin 2.1

            http://boss.bekk.no/boss/middlegen/
            http://www.hibernate.org/
        -->

        <class
                name="com.ulearning.ulms.blog.form.AccountForm"
                table="T_account_Tab"
                >

                <id name="accountId" type="int" column="ACCOUNTID">
                        <generator class="sequence">
                                <param name="sequence">accountId</param>
                        </generator>
                </id>

                <property
                        name="username"
                        type="java.lang.String"
                        column="USERNAME"
                        not-null="true"
                        length="20"
                        />
                <property
                        name="password"
                        type="java.lang.String"
                        column="PASSWORD"
                        not-null="true"
                        length="40"
                        />
                <property
                        name="email"
                        type="java.lang.String"
                        column="EMAIL"
                        length="50"
                        />
                <property
                        name="firstName"
                        type="java.lang.String"
                        column="FIRSTNAME"
                        not-null="true"
                        length="50"
                        />
                <property
                        name="lastName"
                        type="java.lang.String"
                        column="LASTNAME"
                        length="50"
                        />

                <property
                        name="gender"
                        type="int"
                        column="GENDER"
                        not-null="true"
                        length="22"
                        />

                <property
                        name="title"
                        type="java.lang.String"
                        column="TITLE"
                        length="50"
                        />
                <property
                        name="subTitle"
                        type="java.lang.String"
                        column="SUBTITLE"
                        length="50"
                        />
                <property
                        name="createdDate"
                        type="java.util.Date"
                        column="CREATEDDATE"
                        not-null="true"
                        length="7"
                        />
                <property
                        name="state"
                        type="int"
                        column="STATE"
                        length="22"
                        />
                <property
                        name="news"
                        type="java.lang.String"
                        column="NEWS"
                        length="255"
                        />
                <property
                        name="description"
                        type="java.lang.String"
                        column="DESCRIPTION"
                        length="255"
                        />
                <property
                        name="articles"
                        type="int"
                        column="ARTICLES"
                        length="22"
                        />
                <property
                        name="feedbacks"
                        type="int"
                        column="FEEDBACKS"
                        length="22"
                        />
                <property
                        name="maxPerPage"
                        type="int"
                        column="MAXPERPAGE"
                        length="22"
                        />
                <property
                        name="css"
                        type="java.lang.String"
                        column="CSS"
                        length="255"
                        />
                <property
                        name="skinId"
                        type="int"
                        column="SKINID"
                        length="22"
                        />

        </class>
</hibernate-mapping>
