<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
        <class name="com.ulearning.ulms.content.schoolbook.model.MTeachoutstor" table="M_TEACHOUTSTOR">
                <id column="TCOUTID" name="id" type="long">
                        <generator class="sequence"/>
                </id>
                <property column="TCOUTDATE" length="26" name="tcoutdate" type="timestamp"/>
                <property column="TCOUTOPERATOR" length="50" name="tcoutoperator" type="string"/>
                <property column="TCOUTMEMO" length="200" name="tcoutmemo" type="string"/>
                <property column="TCINAUDMARK" length="10" name="tcinaudmark" type="integer"/>
                <property column="TCOUTAUDMAN" length="50" name="tcoutaudman" type="string"/>
                <property column="TCOUTMODE" length="10" name="tcoutmode" type="integer"/>
                <property column="REMARK1" length="50" name="remark1" type="string"/>
                <property column="REMARK2" length="50" name="remark2" type="string"/>
                <property column="REMARK3" length="50" name="remark3" type="string"/>
        </class>
</hibernate-mapping>
