<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>

        <class
                name="com.ulearning.ulms.user.group.model.UserGroupModel"
                table="U_USERGROUP_TAB"
                >

                <id
                        name="usergroupid"
                        column="USERGROUPID"
                        type="java.lang.Integer"
                        length="22"
                        >
                        <generator class="sequence">
                                <param name="sequence">UserGroupID</param>
                        </generator>
                </id>


                <property
                        name="groupid"
                        column="GROUPID"
                        type="java.lang.Integer"
                        length="22"
                        />

                <property
                        name="userid"
                        column="USERID"
                        type="java.lang.Integer"
                        length="22"
                        />

                <property
                        name="remark1"
                        column="REMARK1"
                        type="java.lang.String"
                        length="200"
                        />

                <property
                        name="remark2"
                        column="REMARK2"
                        type="java.lang.String"
                        length="200"
                        />

                <property
                        name="remark3"
                        column="REMARK3"
                        type="java.lang.String"
                        length="200"
                        />

                <property
                        name="remark4"
                        column="REMARK4"
                        type="java.lang.String"
                        length="200"
                        />

        </class>


</hibernate-mapping>
