<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>

        <class
                name="com.ulearning.ulms.admin.gradeterm.model.GradeTermModel"
                table="T_GRADETERM_TAB"
                >

                <id
                        name="gradetID"
                        column="GRADETID"
                        type="java.lang.Integer"
                        length="22"
                        >
                        <generator class="sequence">
                                <param name="sequence">GradeTID</param>
                        </generator>
                </id>


                <property
                        name="grade"
                        column="GRADE"
                        type="java.lang.String"
                        length="50"
                        />

                <property
                        name="term"
                        column="TERM"
                        type="java.lang.String"
                        length="10"
                        />

                <property
                        name="speciality"
                        column="SPECIALITY"
                        type="java.lang.String"
                        length="200"
                        />

                <property
                        name="remark1"
                        column="REMARK1"
                        type="java.lang.String"
                        length="50"
                        />

                <property
                        name="remark2"
                        column="REMARK2"
                        type="java.lang.String"
                        length="50"
                        />

                <property
                        name="remark3"
                        column="REMARK3"
                        type="java.lang.String"
                        length="50"
                        />

        </class>


</hibernate-mapping>
