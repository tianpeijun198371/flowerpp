<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>

        <class
                name="com.ulearning.ulms.admin.gradeuser.model.GradeUserModel"
                table="T_GRADEUSER_TAB"
                >

                <id
                        name="gradeUserID"
                        column="GRADEUSERID"
                        type="java.lang.Integer"
                        length="22"
                        >
                        <generator class="sequence">
                                <param name="sequence">GradeUserID</param>
                        </generator>
                </id>


                <property
                        name="gradeUserName"
                        column="GRADEUSERNAME"
                        type="java.lang.String"
                        length="200"
                        />

                <property
                        name="gradeUserpwd"
                        column="GRADEUSERPWD"
                        type="java.lang.String"
                        length="200"
                        />

                <property
                        name="entergrade"
                        column="ENTERGRADE"
                        type="java.lang.String"
                        length="50"
                        />

                <property
                        name="remark1"
                        column="REMARK1"
                        type="java.lang.String"
                        length="50"
                        />

                <property
                        name="remark2"
                        column="REMARK2"
                        type="java.lang.String"
                        length="50"
                        />

                <property
                        name="remark3"
                        column="REMARK3"
                        type="java.lang.String"
                        length="50"
                        />

        </class>


</hibernate-mapping>
