<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
        <!--
            Created by the Middlegen Hibernate plugin 2.1

            http://boss.bekk.no/boss/middlegen/
            http://www.hibernate.org/
        -->

        <class
                name="com.ulearning.ulms.core.security.model.ModuleModel"
                table="SEC_MODULE_TAB"
                >

                <id name="moduleid" type="int" column="MODULEID">
                        <generator class="sequence">
                                <param name="sequence">MODULEID</param>
                        </generator>
                </id>

                <property
                        name="name"
                        type="java.lang.String"
                        column="NAME"
                        length="50"
                        />
                <property
                        name="sysid"
                        type="int"
                        column="SYSID"
                        length="22"
                        />
                <property
                        name="description"
                        type="java.lang.String"
                        column="DESCRIPTION"
                        length="500"
                        />
        </class>
</hibernate-mapping>
